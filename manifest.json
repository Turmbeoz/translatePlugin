{ "name": "Huh?",
"description": "Google Translate Chrome Extension",
"version": "1.0",
"manifest_version": 2,
"browser_action": {"default_popup": "popup.html"},
"content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js":["main.js"],
    "css": ["styles.css"]
  }
],
"permissions": ["activeTab"],
"background": {    "scripts": ["main.js"]}
}